<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <referenceBlock name="vault.cards.list">
                <block class="OnTap\Tns\Block\Customer\CardRenderer" name="tns.card.renderer" template="Magento_Vault::customer_account/credit_card.phtml">
                    <arguments>
                        <argument name="available_providers" xsi:type="array">
                            <item name="tns_direct" xsi:type="string">tns_direct</item>
                            <item name="tns_hpf" xsi:type="string">tns_hpf</item>
                        </argument>
                    </arguments>
                </block>
            </referenceBlock>
        </referenceContainer>
    </body>
</page>
